{
    "manifest_version": 3,
    "name": "Hide Elements",
    "version": "1.0",
    "description": "An Edge extension to hide specific elements on a website.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }
  